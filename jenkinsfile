pipeline {
    agent any
    parameters{
        string(name : 'name of the person', defaultValue: '',description: 'enter your name')
        choice(name: 'github url', choices: ['githuburl', 'gitlaburl'], description: 'select the url')
    }
  stages {
        stage('param demo') {
            steps {
                echo 'Hi $(params.name)'
            }
        }
    }
}
